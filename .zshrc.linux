# alias
alias ssh="ssh-ident"
alias clip="nc -q0 localhost 8377"
command -v pacman > /dev/null && alias p="yaourt"

export PATH="$PATH:$HOME/.local/bin"

# add ssh keys
eval $([[ -f $HOME/.ssh/bitbucket_rsa ]] && keychain --agents ssh --eval "$HOME/.ssh/bitbucket_rsa")
eval $([[ -f $HOME/.ssh/github_rsa ]] && keychain --agents ssh --eval "$HOME/.ssh/github_rsa")

# Powerline
unset _POWERLINE_SAVE_WIDGET
